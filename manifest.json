{
    "manifest_version": 2,
    "name": "WhatsPopUp",
    "version": "1.0",
    "author": "Miguel Ubilla Rocco",
    "developer": {
      "name": "Miguel Ubilla Rocco",
      "url": "https://github.com/miguel-ubilla/"
    },
  
    "description": "PopUp de WhatsApp en el navegador, para sistemas sin app nativa (linux).",
  
    "homepage_url": "https://github.com/miguel-ubilla/whatspopup",
  
    "applications": {
      "gecko": {
        "id": "whatspopup@agg.cl"
      }
    },
  
    "icons": {
      "96": "icon/whatsapp.svg"
    },
  
    "background": {
        "scripts": ["app.js"]
    },
  
    "browser_action": {
      "default_icon": "icon/whatsapp.svg",
      "default_title": "WhatsPopUp"
    }
  
  }